<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="modelo.Municipio" table="municipio">
		<id name="idMunicipio"  		type="java.lang.Integer"	column="idMunicipio"></id>
		<property name="nomeMunicipio" 	type="java.lang.String" 	column="nomeMunicipio" 	not-null="true" />
		<property name="ufMunicipio" 	type="java.lang.String" 	column="ufMunicipio" 	not-null="true" />
	</class>
</hibernate-mapping>